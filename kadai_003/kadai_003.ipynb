        "# Numpyをインポート\n",
        "import numpy as np\n",
        "# Aの多次元コード\n",
        "A = np.array([[0, 1],\n",
        "              [2, 3],\n",
        "              [4, 5]])\n",
        "# Bの多次元コード\n",
        "B = np.array([[0, 1, 2, 3],\n",
        "              [4, 5, 6, 7]])\n",
        "\n",
        "# 行列の積の計算\n",
        "result = np.dot(A, B)\n",
        "\n",
        "# 計算結果を表示するコード\n",
        "result\n",
        "\n",
        "# 行列の要素の最大値を求めるコード\n",
        "result.max()\n"
      ],
